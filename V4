local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Prison Breaker V4", "Midnight")


local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")

MainSection:NewButton("Tiger Admin", "OP Prison Life Admin", function()
    loadstring(game:HttpGet('https://scriptblox.com/raw/Prison-Life-Tiger-Admin-Gui-10597'))()
end)

MainSection:NewButton("Aliafanboys gui", "Its alright", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/Jk8dDSXw'))()
end)

MainSection:NewButton("Sword", "Get a FE sword!", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/SmartModBoy/script3/main/prisonsword'))()
end)

local Player = Window:NewTab("Player")
local PlayerSection = Player:NewSection("Player")

PlayerSection:NewToggle("Super-Human", "Go fast and jump high.", function(state)
    if state then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 200
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 200
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    end
end)

PlayerSection:NewSlider("WalkSpeed", "Set Custom Walkspeed!", 400, 16, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
 
PlayerSection:NewSlider("JumpPower", "Set Custom Jump Power!", 453, 50, function(s)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
 
PlayerSection:NewButton("Rest WS&JP", "Rest them with a button!", function()
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end)

PlayerSection:NewButton("Fly GUI Mobile", "Fly around the map!", function()
    loadstring(game:HttpGet('https://scriptblox.com/raw/Universal-Script-FLY-GUI-V3-8402'))()
end)

PlayerSection:NewButton("Fly GUI PC", "Fly around the map!", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/C5qp7YeG'))()
end)

PlayerSection:NewButton("Inf Jump GUI", "Jump with no limt!", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/ZsGXfxQZ'))()
end)


local Other = Window:NewTab("Other")
local OtherSection = Other:NewSection("Other")

OtherSection:NewButton("Infinite Yield", "FE Admin Commands", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

OtherSection:NewButton("Chathax", "Be anybody!", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/BT1uban5'))()
end)

local Thanks = Window:NewTab("Thanks")
local ThanksSection = Thanks:NewSection("Thanks to")

ThanksSection:NewLabel("Thanks daslol#6258!")

MainSection:NewLabel("Some things may be patched.")
MainSection:NewLabel("Dont skid me")
MainSection:NewLabel("im da biggest bird!")
MainSection:NewLabel("this is not being updated after.")
